    <!-- Main jumbotron for a primary marketing message or call to action -->


    <div class="jumbotron">
      <div class="clearfix">
        <div class="inner">
          <div class="container">
            <a href="/"><img src="images/logo-01.svg" alt="logo1" id="logo1" class="masthead-brand"></a>
            <nav>
              <ul class="nav masthead-nav">
                <li><a href="#fName">JOIN US</a></li>
                <li><a href="#email_si">SIGN IN</a></li>
              </ul>
            </nav>
          </div>
        </div>
      </div>
      <hr class="drop_shadow">
    <div class="container">
      <div id="browser">
      <img src="images/logo-02.svg" alt="logo2" class="logo2">
        <p>helping Orlando residents </p>
        <p>and visitors to easily find schedule</p>
        <p>for recycling and yard waste</p>
